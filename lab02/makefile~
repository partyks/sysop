#dodatkowy argument nazywa sie LOGS;
#make LOGS=yes
#(tak uaktywnic logowanie na konsole)
#make plik
#(we-wy z plikow)

VPATH=src/include:src/test:src/memlib:src/

CC = gcc $(FLAG) $(INC)
INC = -I src/include
FLAG = -Wall
PROJECT = memlib



main : test.o memlib.o
	$(CC) $^ -o $(PROJECT)

test.o : 
	$(CC) -c test.c -o $@

%.o: %.c
	$(CC) -c $< -o $@

clean :
	rm -f *.o
	rm $(PROJECT)

.PHONY : clean
